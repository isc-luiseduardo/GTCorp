            <div class="item active">
                <img src="~/Content/Carousel/CyberGT/001.jpg" alt="Cyber-GT">
                <div class="carousel-caption">
                    <h3>Cyber-GT</h3>
                    <p>Descripción de la foto de GT Corp.</p>
                </div>
            </div>

            <div class="item">
                <img src="~/Content/Carousel/CyberGT/002.jpg" alt="Cyber-GT">
                <div class="carousel-caption">
                    <h3>Cyber-GT</h3>
                    <p>Descripción de la foto de GT Corp.</p>
                </div>
            </div>

            <div class="item">
                <img src="~/Content/Carousel/CyberGT/003.jpg" alt="Cyber-GT">
                <div class="carousel-caption">
                    <h3>Cyber-GT</h3>
                    <p>Descripción de la foto de GT Corp.</p>
                </div>
            </div>

            <div class="item">
                <img src="~/Content/Carousel/CyberGT/004.jpg" alt="Cyber-GT">
                <div class="carousel-caption">
                    <h3>Cyber-GT</h3>
                    <p>Descripción de la foto de GT Corp.</p>
                </div>
            </div>

            <div class="item">
                <img src="~/Content/Carousel/CyberGT/005.jpg" alt="Cyber-GT">
                <div class="carousel-caption">
                    <h3>Cyber-GT</h3>
                    <p>Descripción de la foto de GT Corp.</p>
                </div>
            </div>
        </div>
        


    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
